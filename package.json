{
  "name": "soli-app",
  "main": "expo-router/entry",
  "version": "0.1.0",
  "packageManager": "yarn@4.5.0",
  "private": true,
  "scripts": {
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "check:tamagui": "tamagui check",
    "start": "expo start -c",
    "android": "expo run:android",
    "release": "expo run:android --variant release",
    "prod": "node scripts/run-demo-autosolve.js --mode=prod",
    "prod:no-timeout": "node scripts/run-demo-autosolve.js --mode=prod --no-prod-timeout",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "harvest:trial": "node scripts/harvest-solvable.js --trial",
    "harvest:100": "node scripts/harvest-solvable.js --target=100",
    "demo:auto-solve": "node scripts/run-demo-autosolve.js",
    "prebuild": "npx expo prebuild --platform android --clean"
  },
  "jest": {
    "preset": "jest-expo",
    "resolver": "jest-pnp-resolver",
    "transformIgnorePatterns": [
      "node_modules/(?!(expo|expo-modules-core|@expo|@react-native|react-native|@react-navigation|@react-native-async-storage|react-native-reanimated)/)"
    ],
    "setupFiles": [
      "<rootDir>/test/setup/mock-expo-winter.ts"
    ],
    "moduleNameMapper": {
      "^expo/winter$": "<rootDir>/test/mocks/expo-winter",
      "^expo/winter/.*": "<rootDir>/test/mocks/expo-winter",
      "^expo/src/winter$": "<rootDir>/test/mocks/expo-winter",
      "^expo/src/winter/.*": "<rootDir>/test/mocks/expo-winter"
    }
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/drawer": "^7.7.2",
    "@react-navigation/native": "^7.1.6",
    "@tamagui/config": "^1.135.4",
    "@tamagui/lucide-icons": "^1.135.4",
    "@tamagui/toast": "^1.135.4",
    "babel-preset-expo": "~13.0.0",
    "burnt": "^0.12.2",
    "expo": "~53.0.23",
    "expo-build-properties": "~0.14.8",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.7",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "jest-expo": "~53.0.10",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "tamagui": "^1.135.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@expo/metro-runtime": "~5.0.5",
    "@tamagui/babel-plugin": "^1.135.4",
    "@tamagui/cli": "^1.135.4",
    "@tamagui/metro-plugin": "^1.135.4",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "jest": "~29.7.0",
    "jest-pnp-resolver": "^1.2.3",
    "typescript": "~5.8.3"
  }
}
