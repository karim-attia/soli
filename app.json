{
  "expo": {
    "name": "Soli",
    "slug": "soli",
    "version": "0.1.2",
    "description": "A polished Klondike Solitaire experience with responsive animations and a solvable shuffle catalog.",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "soli",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b8b3c"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ch.karimattia.soli",
      "buildNumber": "3",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0b8b3c"
      },
      "package": "ch.karimattia.soli",
      "versionCode": 5,
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "backgroundColor": "#0b8b3c",
      "display": "standalone",
      "themeColor": "#0b8b3c"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#416F57",
          "image": "./assets/images/icon.png",
          "imageWidth": 368,
          "resizeMode": "contain"
        }
      ]
    ],
    "newArchEnabled": true,
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9de7c7ee-2f1d-418d-ad51-eee7ede5d609"
      }
    }
  }
}
